@model IEnumerable<XYJPersonalSite.Models.BusinessModels.Tag>

<h3>Top Tags</h3>
<hr />
<div>
    @foreach (var item in Model)
    {
        <a class="btn btn-info tag" asp-controller="Blogs" asp-action="Index" asp-route-tagname="@item.TagName">@item.TagName</a>
    }
</div>
